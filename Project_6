# input the sample text given
input_text = """
NX_3000.12
Your Favorite Gene
Mus musculis

"""
# Split rows
lines = input_text.strip().split("\n")

accession = lines[0]
gene = lines[1]
species = lines[2].replace(" ", "_")
sequence = lines[3]

# Make FASTA header
header = f">{accession} {gene} {species}"

# Print FASTA format
print(header)
print(sequence)

# Create FASTA text
fasta_text = f">{accession} {gene} {species}\n{sequence}"

# Write to FASTA file
with open("output.fasta", "w") as f:
    f.write(fasta_text)

print("Created output.fasta")